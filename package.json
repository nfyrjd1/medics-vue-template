{
  "name": "medics-vue-template",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "set-node": "nvm use 23",
    "app-install": "nvm install 23 && npm run set-node && npm ci",
    "serve": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "lf": "npm run lint & npm run format",
    "check-updates": "npx npm-check-updates"
  },
  "dependencies": {
    "@medics/medics-vue-components": "^1.1.1",
    "@vueuse/core": "^12.7.0",
    "axios": "^1.8.1",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.27.0",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.8",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "~5.8.2",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
